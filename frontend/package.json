{
  "name": "school",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --aot=false",
    "build:prod": "ng build --prod --aot=false",
    "test": "ng test",
    "test:coverage": "nyc ng test --cc --single run && http-server -c-1 -o -p 9875 ./coverage",
    "e2e": "ng e2e",
    "lint": "ng lint --format=stylish",
    "lintScss": "sass-lint ./ ./src/**/*.scss -v --no-exit --max-warnings=1",
    "commitmsg": "node validate-commit-message.js",
    "precommit": "ng lint && npm run lintScss && ng build",
    "prepush": "ng test --cc --single run && ng e2e && ng lint --type-check && npm run lintScss",
    "doc": "./node_modules/.bin/compodoc -p ./tsconfig.json && http-server -c-1 -o -p 9876 ./documentation"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.2",
    "@angular/common": "^4.2.5",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.3.2",
    "@angular/forms": "^4.3.2",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.2.5",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular2-image-upload": "^1.0.0-rc.1",
    "angular2-text-mask": "^8.0.4",
    "angular4-carousel": "^3.1.3",
    "ckeditor": "^4.7.3",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "html-pdf-viewer": "^1.0.14",
    "moment": "^2.19.2",
    "ng2-ckeditor": "^1.1.9",
    "ng2-date-picker": "^2.6.2",
    "ng2-datepicker": "^1.8.3",
    "ng2-img-max": "2.1.6",
    "rxjs": "^5.1.0",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.5",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "http-server": "^0.9.0",
    "husky": "^0.13.3",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-istanbul-threshold": "^1.2.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nyc": "^10.3.0",
    "protractor": "~5.1.0",
    "sass-lint": "^1.10.2",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
